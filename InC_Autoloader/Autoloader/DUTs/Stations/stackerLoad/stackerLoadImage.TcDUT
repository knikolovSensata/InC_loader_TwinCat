									   iType  		  := ioType.OUTPUT_BIT,
									   iByte 		  := 151,
									   iBit   		  := 5 ); 
									   
    yPalletFix_unlock 				: imageIO := ( stIO   := Stations.sLoadHmiIO.y_PalletFix_unlock,
									   image  		  := ADR(Global.OUTPUT_IMAGE),
									   iType  		  := ioType.OUTPUT_BIT,
									   iByte 		  := 151,
									   iBit   		  := 4 ); 							    

